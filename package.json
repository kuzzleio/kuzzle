{
  "name": "kuzzle",
  "author": "The Kuzzle Team <support@kuzzle.io>",
  "version": "2.10.1",
  "description": "Kuzzle is an open-source solution that handles all the data management through a secured API, with a large choice of protocols.",
  "bin": {
    "kuzzle": "bin/start-kuzzle-server"
  },
  "scripts": {
    "file": "node docker/scripts/reloader.js --inspect=0.0.0.0:9229 -r ts-node/register ",
    "start:dev": "node docker/scripts/reloader.js --inspect=0.0.0.0:9229 -r ts-node/register docker/scripts/start-kuzzle-dev.ts --enable-plugins functional-test-plugin",
    "prepublishOnly": "npm run build",
    "build": "npm run build-ts",
    "clean": "touch index.ts && npm run build | grep TSFILE | cut -d' ' -f 2 | xargs rm",
    "build-ts": "tsc",
    "services": "npx kourou app:start-services",
    "dev": "node docker/scripts/reloader.js --inspect=0.0.0.0:9229 -r ts-node/register ",
    "dev:test": "npm run dev -- docker/scripts/start-kuzzle-dev.ts --enable-plugins functional-test-plugin",
    "test": "npm run --silent test:lint && npm run test:unit:coverage && npm run test:functional",
    "test:unit": "DEBUG= npx --node-arg=--trace-warnings mocha",
    "test:unit:coverage": "DEBUG= nyc --reporter=text-summary --reporter=lcov mocha",
    "test:functional": "npm run test:functional:http && npm run test:functional:websocket",
    "test:functional:http": "KUZZLE_PROTOCOL=http npx cucumber-js --profile http",
    "test:functional:websocket": "KUZZLE_PROTOCOL=websocket npx cucumber-js --profile websocket",
    "test:functional:legacy": "npm run test:functional:legacy:http && npm run test:functional:legacy:websocket && npm run test:functional:legacy:mqtt && npm run test:functional:legacy:cluster",
    "test:functional:legacy:http": "npx cucumber-js --format progress-bar --profile http ./features-legacy",
    "test:functional:legacy:websocket": "npx cucumber-js --format progress-bar --profile websocket ./features-legacy",
    "test:functional:legacy:mqtt": "npx cucumber-js --format progress-bar --profile mqtt ./features-legacy",
    "test:functional:legacy:cluster": "npx cucumber-js --format progress-bar --profile websocketCluster ./features-legacy",
    "cucumber": "cucumber.js --fail-fast",
    "codecov": "codecov",
    "test:lint": "npm run test:lint:js && npm run test:lint:ts",
    "test:lint:ts": "eslint --max-warnings=0 ./lib --ext .ts --config .eslintc-ts.json",
    "test:lint:ts:fix": "eslint --max-warnings=0 --fix ./lib --ext .ts --config .eslintc-ts.json",
    "test:lint:js": "eslint --max-warnings=0 ./lib ./test ./features-legacy ./bin ./features ./doc/build-error-codes.js ./plugins/available/functional-test-plugin",
    "test:lint:js:fix": "eslint --max-warnings=0 --fix ./lib ./test ./features-legacy ./bin ./features ./doc/build-error-codes.js",
    "doc-prepare": "kuzdoc framework:install",
    "doc-dev": "kuzdoc repo:dev -d /core/2/ -v 2",
    "doc-build": "kuzdoc repo:build -d /core/2/ -v 2",
    "doc-upload": "kuzdoc repo:deploy -d /core/2/ -v 2",
    "doc-cloudfront": "kuzdoc repo:cloudfront -d /core/2/*",
    "doc-deploy": "npm run doc-upload && npm run doc-cloudfront",
    "doc-netlify": "npm run doc-prepare && kuzdoc repo:build -d / -v 2",
    "doc-error-codes": "node -r ts-node/register doc/build-error-codes"
  },
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.11.0",
    "aedes": "^0.44.2",
    "bluebird": "^3.7.2",
    "busboy": "^0.3.1",
    "cli-color": "^2.0.0",
    "cookie": "^0.4.1",
    "debug": "^4.3.1",
    "denque": "^1.5.0",
    "didyoumean": "^1.2.1",
    "dumpme": "^1.0.3",
    "eventemitter3": "^4.0.7",
    "inquirer": "^7.3.3",
    "ioredis": "^4.23.0",
    "ip": "^1.1.5",
    "json-stable-stringify": "^1.0.1",
    "json2yaml": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "koncorde": "^3.0.0",
    "kuzzle-plugin-auth-passport-local": "^6.2.0",
    "kuzzle-plugin-logger": "^3.0.2",
    "kuzzle-sdk": "7.5.6",
    "kuzzle-vault": "^2.0.4",
    "lodash": "4.17.21",
    "moment": "^2.29.1",
    "ms": "^2.1.3",
    "murmurhash-native": "^3.5.0",
    "passport": "^0.4.1",
    "rc": "1.2.8",
    "semver": "^7.3.4",
    "sorted-array": "^2.0.4",
    "uuid": "^8.3.2",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v18.4.0",
    "validator": "^13.5.2",
    "winston": "^3.3.3",
    "winston-elasticsearch": "0.14.0",
    "winston-syslog": "^2.4.4",
    "winston-transport": "^4.4.0",
    "yargs": "^16.2.0",
    "zeromq": "^5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kuzzleio/kuzzle.git"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.168",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "async": "^3.2.0",
    "chokidar": "^3.5.1",
    "codecov": "^3.8.1",
    "cucumber": "^6.0.5",
    "eslint": "^7.20.0",
    "js-yaml": "^4.0.0",
    "kuzdoc": "^1.4.5",
    "mocha": "^8.3.0",
    "mock-require": "^3.0.3",
    "mqtt": "^4.2.6",
    "nyc": "^15.1.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "rewire": "^5.0.0",
    "should": "13.2.3",
    "should-sinon": "0.0.6",
    "sinon": "^9.2.4",
    "strip-json-comments": "^3.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2"
  },
  "engines": {
    "node": ">= 12.13.0"
  },
  "license": "Apache-2.0",
  "files": [
    "bin/**/*.js",
    "bin/**/*.d.ts",
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "lib/**/*.json",
    "plugins/**/*.js",
    "plugins/**/*.d.ts",
    "plugins/**/*.json",
    "plugins/**/*.lua",
    "plugins/**/LICENSE.md",
    "plugins/**/README.md",
    "plugins/**/.keep",
    "protocols/**/*.js",
    "protocols/**/*.d.ts",
    "protocols/**/*.json",
    "protocols/**/LICENSE.md",
    "protocols/**/README.md",
    "protocols/**/.keep",
    "package.json",
    "package-lock.json",
    "index.js",
    "index.d.ts",
    ".kuzzlerc.sample",
    "LICENSE.md",
    "CONTRIBUTING.md",
    "README.md"
  ]
}
