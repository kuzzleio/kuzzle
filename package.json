    "bluebird": "^3.5.5",
    "bufferutil": "^4.0.1",
    "busboy": "^0.3.0",
    "bytes": "^3.1.0",
    "cli-color": "^1.4.0",
    "commander": "^2.19.0",
    "debug": "^4.1.1",
    "denque": "^1.4.0",
    "didyoumean": "^1.2.1",
    "dumpme": "^1.0.2",
    "easy-circular-list": "^1.0.13",
    "elasticsearch": "^15.5.0",
    "eventemitter3": "^3.1.2",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.4",
    "ioredis": "^4.10.4",
    "json-stable-stringify": "1.0.1",
    "json2yaml": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "koncorde": "^1.2.1",
    "kuzzle-common-objects": "^3.0.15",
    "kuzzle-sdk": "^6.1.3",
    "lodash": "4.17.11",
    "moment": "^2.24.0",
