{
  "kuzzle-plugin-logger": {
    "version": "1.0.6",
    "activated": true,
    "name": "kuzzle-plugin-logger",
    "defaultConfig": {
      "service": "winston",
      "level": "info",
      "addDate": true,
      "loadedBy": "all"
    }
  },
  "kuzzle-plugin-socketio": {
    "version": "1.0.6",
    "activated": true,
    "name": "kuzzle-plugin-socketio",
    "defaultConfig": {
      "port": 7512,
      "room": "kuzzle",
      "loadedBy": "server"
    }
  },
  "kuzzle-plugin-auth-passport-local": {
    "version": "1.3.4",
    "activated": true,
    "name": "kuzzle-plugin-auth-passport-local",
    "defaultConfig": {
      "secret": "changeme",
      "algorithm": "sha1",
      "digest": "hex"
    }
  }
}